<div class='container'>
  <br />
  <div class='card'>
    <div class='card-body'>
      <div class='card'>
        <div class='card-body'>
          <h1>{{heading.main.title}}</h1>
          <p>{{heading.main.text}}</p>
          <div><button
              class='btn btn-primary'
            >{{heading.main.button_text.solid}}</button></div>
          <br />
          <div><button
              class='btn btn-outline-primary'
            >{{heading.main.button_text.outline}}</button></div>
          <br />
        </div>
      </div>
      <br />

      <div class='card'>
        <div class='card-body'>
          <h2>{{heading.second.title}}</h2>
          <p>{{heading.second.text}}</p>
          <div><button
              class='btn btn-secondary'
            >{{heading.second.button_text.solid}}</button></div>
          <br />
          <div><button
              class='btn btn-outline-secondary'
            >{{heading.second.button_text.outline}}</button></div>
          <br />
        </div>
      </div>
      <br />
      <div class='card'>
        <div class='card-body'>
          <h3>{{heading.subblock.title}}</h3>
          <p>{{heading.subblock.text}}</p>
          <div>
            <button
              class='btn btn-dark'
            >{{heading.subblock.button_text.solid}}</button>
          </div>
          <br />
          <button
            class='btn btn-outline-dark'
          >{{heading.subblock.button_text.outline}}</button>
          <br />
        </div>
      </div>

    </div>
  </div>
  <br />
  <br />
  <div class='card'>
    <div class='card-body'>
      <div class='display-1'>
        {{nested_display.title}}
      </div>
      <br />
      <div class='card col-8'>
        <div class='card-body'>
          <div
            class='display-4'
          >{{nested_display.children_display.title}}</div>
          <p>{{nested_display.children_display.text}}</p>
          <button
            class='btn btn-outline-warning'
          >{{nested_display.children_display.button.text}}</button>
        </div>
      </div>
      <br />
      <h6>{{nested_display.header_text}}</h6>
      <button
        class='btn btn-info'
      >{{nested_display.button.text}}</button>
    </div>
  </div>
  <br />
  <br />
  <div class='card'>
    <div class='card-body'>
      <p class='text-uppercase'>{{paragraph.title}}</p>
      <p class='text-lowercase'>{{paragraph.text}}</p>
      <button class='btn btn-link'><a
          href='{{paragraph.button.url}}'
        >{{paragraph.button.text}}</a></button>
      <br />
      <br />
      <div class='card'>
        <div class='card-body'>
          <ul>
            <li><button class='btn btn-outline-light'><a
                  href='https://github.com/'
                >Github</a></button></li>
            <br />
            <li><button class='btn btn-outline-light'><a
                  href='https://getbootstrap.com/'
                >Bootstrap</a></button></li>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <br />
</div>