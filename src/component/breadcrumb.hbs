<section class="py-4">
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            {{#each breadcrumb}}
                <li class="breadcrumb-item">
                    <a class="breadcrumb-item text-decoration-none {{#if @last}}active{{/if}}" href="{{#if url}}{{url}}{{/if}}">{{name}}</a>
                </li>
            {{/each}} 
        </ol>
    </nav>
</section> 