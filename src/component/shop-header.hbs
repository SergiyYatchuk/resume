<nav>
  <div class='row'>
    <div class='col-12'>
      <div class='card p-3'>
        {{#with navigation}}
          <div class='nav'>
            <div class='col-8'>
              <div class='nav-item'>
                <h4 class='card-title'>{{Logo}}</h4>
              </div>
            </div>
            <div class='col-4'>
              <div class='nav justify-content-end'>
                {{#each links}}
                  <div class='nav-item'>
                    <a
                      href='{{href}}'
                      class='nav-link text-end'
                    >
                      {{text}}
                    </a>
                  </div>
                {{/each}}
              </div>
            </div>
          </div>
        {{/with}}
      </div>
    </div>
  </div>
</nav>